{
  "name": "Auto CForm",
  "version": "0.1.0",
  "description": "Automatically Fill CForm",
  "icons": { 
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
   "128": "icons/icon_128.png" },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Auto CForm"

  },
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3
}